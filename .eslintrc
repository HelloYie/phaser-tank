{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "no-console": 0,
        "indent": [2, 2, {"SwitchCase": 1}],
        "arrow-body-style": 0,
        "eol-last": 0,
        "max-len": [2, 150, 2, {"ignoreComments": true, "ignoreUrls": true}],
        "camelcase": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "no-undef": 0,
        "no-new": 0,
        "func-names": 0,
        "import/prefer-default-export": 0,
        "no-param-reassign": 0,
        "import/imports-first": 0,
    },
    "plugins": [],
}
